<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OverScrollView">
        <attr name="refreshHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="EduSohoTextBtn">
        <attr name="text" format="string" />
        <attr name="image" format="string" />
        <attr name="size" format="dimension" />
        <attr name="color" format="color" />
    </declare-styleable>
    <declare-styleable name="CustomShapeImageView">
        <attr name="shape" format="integer">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="svg" value="3" />
        </attr>

        <attr name="svg_raw_resource" format="reference" />

    </declare-styleable>

    <declare-styleable name="CourseListWidget">
        <attr name="showMoreBtn" format="boolean" />
    </declare-styleable>
</resources>
