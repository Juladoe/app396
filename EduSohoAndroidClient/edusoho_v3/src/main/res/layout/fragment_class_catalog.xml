<?xml version="1.0" encoding="utf-8"?>
<com.edusoho.kuozhi.v3.view.headStopScroll.StopScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:scrollbars="none">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.edusoho.kuozhi.v3.view.FixHeightListView
            android:id="@+id/lv_catalog"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/common_margin"
            android:layout_marginRight="@dimen/common_margin"
            android:divider="@color/disabled_hint_color"
            android:dividerHeight="1dp"
            android:scrollbars="none" />

        <com.edusoho.kuozhi.v3.view.InnerLoadView
            android:id="@+id/il_class_catalog_load"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </RelativeLayout>

</com.edusoho.kuozhi.v3.view.headStopScroll.StopScrollView>